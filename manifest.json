{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Mokcha",
    "action": {
        "default_popup": "index.html",
        "default_title": "Mokcha"
    },
    "host_permissions": ["http://*/*", "https://*/*", "*://developer.mozilla.org/*"],
    "permissions": [
        "webRequest"
    ],
    "background": {
        "service_worker": "background.js"
    }
}